<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/FirstFragment">

    <fragment
        android:id="@+id/FirstFragment"
        android:name="com.andy312.linuxsandroid.FirstFragment"
        android:label="MainFragment"
        tools:layout="@layout/fragment_first">
        <action
            android:id="@+id/action_to_addnew"
            app:destination="@id/navigation"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim" />
    </fragment>
    <navigation android:id="@+id/navigation"
        app:startDestination="@id/new_config">
        <fragment
            android:id="@+id/new_config"
            android:name="com.andy312.linuxsandroid.new_config"
            android:label="fragment_new_config"
            tools:layout="@layout/fragment_new_config" />
    </navigation>
</navigation>